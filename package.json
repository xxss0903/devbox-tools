{
  "name": "devbox-tools",
  "version": "0.0.0",
  "private": true,
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "electron:serve": "npm run compile:electron && concurrently \"vite dev\" \"wait-on http://localhost:5173 && electron .\"",
    "electron:build": "vue-tsc && vite build && npm run compile:electron && electron-builder",
    "compile:electron": "tsc -p electron",
    "postinstall": "electron-rebuild -f -w sqlite3"
  },
  "dependencies": {
    "@vueup/vue-quill": "^1.2.0",
    "crypto-js": "^4.2.0",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "quill": "^2.0.2",
    "sequelize": "^6.32.1",
    "sqlite": "^4.1.2",
    "sqlite3": "^5.1.6",
    "v-calendar": "^3.1.2",
    "vue": "^3.4.29",
    "vue-advanced-cropper": "^2.8.8",
    "vue-router": "^4.4.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.8.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.14.5",
    "@types/sqlite3": "^3.1.8",
    "@vitejs/plugin-vue": "^5.0.5",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/tsconfig": "^0.5.1",
    "concurrently": "^8.2.0",
    "electron": "^32.0.2",
    "electron-builder": "^25.0.5",
    "electron-devtools-installer": "^3.2.0",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "npm-run-all2": "^6.2.0",
    "prettier": "^3.2.5",
    "typescript": "~5.4.0",
    "vite": "^5.3.1",
    "vite-plugin-vue-devtools": "^7.3.1",
    "vue-tsc": "^2.0.21",
    "wait-on": "^7.0.1"
  }
}
